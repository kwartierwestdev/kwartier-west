{
  "redirects": [
    {
      "source": "/pages/hiphop/artist.html",
      "has": [
        {
          "type": "query",
          "key": "slug",
          "value": "(?<slug>[^&]+)"
        }
      ],
      "destination": "/pages/hiphop/artist/:slug",
      "permanent": true
    },
    {
      "source": "/pages/tekno/artist.html",
      "has": [
        {
          "type": "query",
          "key": "slug",
          "value": "(?<slug>[^&]+)"
        }
      ],
      "destination": "/pages/tekno/artist/:slug",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/pages/hiphop/artist/:slug",
      "destination": "/pages/hiphop/artist.html?slug=:slug"
    },
    {
      "source": "/pages/hiphop/artist/:slug/",
      "destination": "/pages/hiphop/artist.html?slug=:slug"
    },
    {
      "source": "/pages/tekno/artist/:slug",
      "destination": "/pages/tekno/artist.html?slug=:slug"
    },
    {
      "source": "/pages/tekno/artist/:slug/",
      "destination": "/pages/tekno/artist.html?slug=:slug"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        }
      ]
    }
  ]
}
